apiVersion: apps/v1
kind: Deployment
metadata:
  name: pjtc-dev-api
  namespace: pjtc-dev-api-1
spec:
  replicas: 1
  template:
    metadata:
      labels:
        app: pjtc-dev-api
spec:
    containers:
    - name: pjtc-dev-api
      image: 364650154543.dkr.ecr.ap-northeast-2.amazonaws.com/member-api:10
      ports:
        - containerPort: 8080

apiVersion: apps/v1
kind: Deployment
metadata:
  name: pjtc-dev-api-deployment
spec:
  replicas: 1
  selector:
    matchLabels:
      app: pjtc-dev-api
  template:
    metadata:
      labels: 
        app: pjtc-dev-api
    spec:     # pod spec                  
      containers: 
      - name: pjtc-dev-api
        image: 364650154543.dkr.ecr.ap-northeast-2.amazonaws.com/member-api:10
        ports:
        - containerPort: 8080
